{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Pub Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ESNext", "ES2015.Proxy", "dom"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
